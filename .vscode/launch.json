  "configurations": [
    {
      "name": "run dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--dart-define=FLAVOR=dev"]
    },
    {
      "name": "run dev (with Device Preview)",
